{
    "name": "patinthehat/musicbrainz",
    "type": "library",
    "description": "A PHP library for accessing the MusicBrainz API, updated for php7",
    "keywords": ["musicbrainz", "library", "php", "api"],
    "homepage": "https://github.com/patinthehat/musicbrainz",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Almond",
            "email": "mike.almond@gmail.com",
            "homepage": "http://mikealmond.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "suggest": {
        "guzzle/guzzle": "Allows you to use the GuzzleHttpAdapter. You must install one of the supported adapters.",
        "rmccue/requests": "Allows you to use the RequestsHttpAdapter. You must install one of the supported adapters."
    },
    "autoload": {
        "psr-4": {
            "MusicBrainz\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
